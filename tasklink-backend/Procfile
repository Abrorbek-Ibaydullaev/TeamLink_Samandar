release: python manage.py migrate && python manage.py shell -c "from django.contrib.auth import get_user_model; User = get_user_model(); User.objects.filter(username='admin').exists() or User.objects.create_superuser('admin', 'admin@teamlink.com', 'Admin@123')"
web: gunicorn config.wsgi:application --bind 0.0.0.0:$PORT --workers 2 --timeout 120